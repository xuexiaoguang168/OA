        <alias>oa</alias>
        <driver-url>jdbc:mysql://localhost:3306/redmoonoa?useUnicode=true&amp;characterEncoding=UTF-8&amp;zeroDateTimeBehavior=convertToNull</driver-url>
        <driver-class>com.mysql.jdbc.Driver</driver-class>
